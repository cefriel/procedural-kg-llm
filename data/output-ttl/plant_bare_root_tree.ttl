@prefix po: <http://example.org/procedure-ontology#> . 

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . 

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . 

@prefix ex: <http://example.org#> . 

# Procedure 

ex:PlantBareRootTree 

    a po:Procedure ; 

    rdfs:label "Plant a Bare Root Tree"^^rdfs:Literal ; 

    po:hasStep ex:PlantBareRootTree_step1 , ex:PlantBareRootTree_step2 , ex:PlantBareRootTree_step3 , ex:PlantBareRootTree_step4 , ex:PlantBareRootTree_step5 , ex:PlantBareRootTree_step6 , ex:PlantBareRootTree_step7 , ex:PlantBareRootTree_step8 , ex:PlantBareRootTree_step9 , ex:PlantBareRootTree_step10 , ex:PlantBareRootTree_step11 , ex:PlantBareRootTree_step12 , ex:PlantBareRootTree_step13 , ex:PlantBareRootTree_step14 , ex:PlantBareRootTree_step15 , ex:PlantBareRootTree_step16 , ex:PlantBareRootTree_step17 , ex:PlantBareRootTree_step18 . 

  

# Steps 

ex:PlantBareRootTree_step1  

    a po:Step ; 

    rdfs:label "Carefully unpack the bare root tree from the container or material it came in."^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:stepNumber "1"^^rdfs:Literal ; 

    po:hasAction ex:Unpack ; 

    po:hasTool ex:Container ; 

    po:hasNextStep ex:PlantBareRootTree_step2 . 

  

ex:PlantBareRootTree_step2  

    a po:Step ; 

    rdfs:label "If you do damage a few, nip them off with a pair of sterilized pruners."^^rdfs:Literal ; 

    po:stepNumber "2"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:NipOff ; 

    po:hasTool ex:PairOfSterilizedPruners ; 

    po:hasNextStep ex:PlantBareRootTree_step3 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step1 . 

  

ex:PlantBareRootTree_step3  

    a po:Step ; 

    rdfs:label "Set the tree into a bucket filled with water."^^rdfs:Literal ; 

    po:stepNumber "3"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Set ; 

    po:hasTool ex:Bucket ; 

    po:hasNextStep ex:PlantBareRootTree_step4 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step2 . 

  

ex:PlantBareRootTree_step4  

    a po:Step ; 

    rdfs:label "Allow the tree to soak for 4-6 hours prior to planting."^^rdfs:Literal ; 

    po:stepNumber "4"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:AllowToSoak ; 

    po:hasNextStep ex:PlantBareRootTree_step5 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step3 . 

  

ex:PlantBareRootTree_step5  

    a po:Step ; 

    rdfs:label "Dig a hole slightly larger than the diameter and depth of the tree and soil width."^^rdfs:Literal ; 

    po:stepNumber "5"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Dig ; 

    po:hasNextStep ex:PlantBareRootTree_step6 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step4 . 

  

ex:PlantBareRootTree_step6  

    a po:Step ; 

    rdfs:label "Check to be sure there are no large weed roots in the hole you have dug."^^rdfs:Literal ; 

    po:stepNumber "6"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Check ; 

    po:hasNextStep ex:PlantBareRootTree_step7 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step5 . 

  

ex:PlantBareRootTree_step7  

    a po:Step ; 

    rdfs:label "Add your organic material to the hole and mix well."^^rdfs:Literal ; 

    po:stepNumber "7"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Add , ex:Mix ; 

    po:hasNextStep ex:PlantBareRootTree_step8 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step6 . 

  

ex:PlantBareRootTree_step8  

    a po:Step ; 

    rdfs:label "Shovel the remaining dirt from the container."^^rdfs:Literal ; 

    po:stepNumber "8"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Shovel ; 

    po:hasNextStep ex:PlantBareRootTree_step9 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step7 . 

  

ex:PlantBareRootTree_step9  

    a po:Step ; 

    rdfs:label "Add more if necessary into the hole, taking care to pack the soil firmly around the tree."^^rdfs:Literal ; 

    po:stepNumber "9"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Add , ex:Pack ; 

    po:hasNextStep ex:PlantBareRootTree_step10 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step8 . 

  

ex:PlantBareRootTree_step10  

    a po:Step ; 

    rdfs:label "Build a water basin around the outside of the tree."^^rdfs:Literal ; 

    po:stepNumber "10"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Build ; 

    po:hasNextStep ex:PlantBareRootTree_step11 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step9 . 

  

ex:PlantBareRootTree_step11  

    a po:Step ; 

    rdfs:label "Give the tree plenty of water."^^rdfs:Literal ; 

    po:stepNumber "11"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Give ; 

    po:hasNextStep ex:PlantBareRootTree_step12 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step10 . 

  

ex:PlantBareRootTree_step12  

    a po:Step ; 

    rdfs:label "Add a mulch area of a meter/yard wide and 5 centimeters (2.0 in) deep around the tree base."^^rdfs:Literal ; 

    po:stepNumber "12"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Add ; 

    po:hasNextStep ex:PlantBareRootTree_step13 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step11 . 

  

ex:PlantBareRootTree_step13  

    a po:Step ; 

    rdfs:label "Water the tree every two weeks throughout its first summer."^^rdfs:Literal ; 

    po:stepNumber "13"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Water ; 

    po:hasNextStep ex:PlantBareRootTree_step14 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step12 . 

  

ex:PlantBareRootTree_step14  

    a po:Step ; 

    rdfs:label "If your area is experiencing a heavy long-term drought, water your young trees in the winter about every 2 weeks."^^rdfs:Literal ; 

    po:stepNumber "14"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Water ; 

    po:hasNextStep ex:PlantBareRootTree_step15 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step13 . 

  

ex:PlantBareRootTree_step15  

    a po:Step ; 

    rdfs:label "Stake large trees."^^rdfs:Literal ; 

    po:stepNumber "15"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Stake ; 

    po:hasNextStep ex:PlantBareRootTree_step16 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step14 . 

  

ex:PlantBareRootTree_step16  

    a po:Step ; 

    rdfs:label "Then tie the tree stem to the stake with a rubber tree tie."^^rdfs:Literal ; 

    po:stepNumber "16"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Tie ; 

    po:hasTool ex:RubberTreeTie ; 

    po:hasNextStep ex:PlantBareRootTree_step17 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step15 . 

  

ex:PlantBareRootTree_step17  

    a po:Step ; 

    rdfs:label "Remove the stake after a year."^^rdfs:Literal ; 

    po:stepNumber "17"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Remove ; 

    po:hasNextStep ex:PlantBareRootTree_step18 ; 

    po:hasPreviousStep ex:PlantBareRootTree_step16 . 

  

ex:PlantBareRootTree_step18  

    a po:Step ; 

    rdfs:label "Remove the rubber tree tie, and saw off the stake at soil level."^^rdfs:Literal ; 

    po:stepNumber "18"^^rdfs:Literal ; 

    po:isStepOf ex:PlantBareRootTree ; 

    po:hasAction ex:Remove , ex:Saw ; 

    po:hasTool ex:Saw ; 

    po:hasPreviousStep ex:PlantBareRootTree_step17 . 

  

# Tools 

ex:Container 

    a po:Tool ; 

    rdfs:label "Container."^^rdfs:Literal ; 

    po:isToolOf ex:PlantBareRootTree_step1 . 

  

ex:PairOfSterilizedPruners 

    a po:Tool ; 

    rdfs:label "Pair of sterilized pruners."^^rdfs:Literal ; 

    po:isToolOf ex:PlantBareRootTree_step2 . 

  

ex:Bucket 

    a po:Tool ; 

    rdfs:label "Bucket."^^rdfs:Literal ; 

    po:isToolOf ex:PlantBareRootTree_step3 . 

  

ex:RubberTreeTie 

    a po:Tool ; 

    rdfs:label "Rubber tree tie."^^rdfs:Literal ; 

    po:isToolOf ex:PlantBareRootTree_step16 . 

  

ex:Saw 

    a po:Tool ; 

    rdfs:label "Saw."^^rdfs:Literal ; 

    po:isToolOf ex:PlantBareRootTree_step18 . 

  

# Actions 

ex:Unpack 

    a po:Action ; 

    rdfs:label "Unpack."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step1 . 

  

ex:NipOff 

    a po:Action ; 

    rdfs:label "Nip off."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step2 . 

  

ex:Set 

    a po:Action ; 

    rdfs:label "Set."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step3 . 

  

ex:AllowToSoak 

    a po:Action ; 

    rdfs:label "Allow to soak."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step4 . 

  

ex:Dig 

    a po:Action ; 

    rdfs:label "Dig."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step5 . 

  

ex:Check 

    a po:Action ; 

    rdfs:label "Check."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step6 . 

  

ex:Add 

    a po:Action ; 

    rdfs:label "Add."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step7 , ex:PlantBareRootTree_step12 , ex:PlantBareRootTree_step14 , ex:PlantBareRootTree_step18 . 

  

ex:Mix 

    a po:Action ; 

    rdfs:label "Mix."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step7 . 

  

ex:Shovel 

    a po:Action ; 

    rdfs:label "Shovel."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step8 . 

  

ex:Pack 

    a po:Action ; 

    rdfs:label "Pack."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step9 . 

  

ex:Build 

    a po:Action ; 

    rdfs:label "Build."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step10 . 

  

ex:Give 

    a po:Action ; 

    rdfs:label "Give."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step11 . 

  

ex:Water 

    a po:Action ; 

    rdfs:label "Water."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step13 , ex:PlantBareRootTree_step14 . 

  

ex:Stake 

    a po:Action ; 

    rdfs:label "Stake."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step15 . 

  

ex:Tie 

    a po:Action ; 

    rdfs:label "Tie."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step16 . 

  

ex:Remove 

    a po:Action ; 

    rdfs:label "Remove."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step17 , ex:PlantBareRootTree_step18 . 

  

ex:Saw 

    a po:Action ; 

    rdfs:label "Saw."^^rdfs:Literal ; 

    po:isActionOf ex:PlantBareRootTree_step18 . 